@model torre.Models.MapViewModel

<div class="row">
    <div id="map" class="map col-lg-12"></div>
</div>

@section scripts
{
    <script src="@Url.Content("~/Scripts/ViewModels/Home/Index.js")"></script>
    <script>
        var viewModel =
            new torre.ViewModels.Home.Index({ Latitude: @(Model.Center.Latitude), Longitude: @(Model.Center.Longitude) },
                @(Model.Zoom));
    </script>
}